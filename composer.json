{
    "name": "d4rk0snet/fiscalreceipt",
    "description": "Plugin métier pour la gestion des recus fiscaux au sein de l'association de préservation des coraux Coral-Guardian",
    "type": "wordpress-plugin",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "D4rk0snet\\FiscalReceipt\\": "src/"
        }
    },
    "repositories": [
        {
            "type" : "vcs",
            "url": "git@github.com:ds-hyperion/api2pdf.git",
            "no-api": true
        },
        {
            "type" : "vcs",
            "url": "git@github.com:ds-hyperion/restapi.git",
            "no-api": true
        }
    ],
    "authors": [
        {
            "name": "Grégory COLLIN",
            "email": "gregory@d4rk0s.net"
        }
    ],
    "require": {
        "twig/twig": "^3.3",
        "hyperion/api2pdf": "dev-dev",
        "hyperion/restapi": "dev-dev",
        "ext-intl": "*"
    }
}
